@model List<RPGMaker.Models.Index.ClassViewModel>

@{
    ViewData["Title"] = "Classes";
}

<h1>Classes</h1>

<p>
    <a asp-action="Create" class="btn btn-default">Create new</a>
</p>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Name</th>

    </tr>
    @foreach (RPGMaker.Models.Index.ClassViewModel item in Model)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Name</td>
        </tr>
    }
</table>